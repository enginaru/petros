<body>


   
<div class="container">
    <h2>성경헬라어(한천설) ch7 단어 </h2>
    <table class="table table-striped" id="mytable">
        <thead>
        <tr>
            <th>ID</th>
            <th>title</th>
            <th>name</th>
        </tr>
        </thead>
        <tbody>
        {{#each results}}
	    {{#compare chapter '=='7 }}
            <tr>
                <td>{{ id }}</td>
                <td>{{ title }}</td>
                <td>{{ name }}</td>
            </tr>
	    {{/compare}}

        {{/each}}
        </tbody>
    </table>

  <h3>단어확인</h3>

    <table class="table table-striped" id="mytable2" style="display: block;">
        <thead>
        <tr>
            <th>ID</th>
            <th>title</th>
        </tr>
        </thead>
        <tbody>
        {{#each results}}
	    {{#compare chapter '=='7 }}
            <tr>
                <td>{{ id }}</td>
                <td>{{ title }}</td>
            </tr>
	    {{/compare}}

        {{/each}}
        </tbody>
    </table>

</div> 


